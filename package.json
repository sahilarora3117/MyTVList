{
  "name": "mytvlist",
  "version": "0.1.0",
  "description": "A minimal Electron based application for tracking TV Shows",
  "private": true,
  "homepage": ".",
  "author": {
    "name": "Sahil Arora",
    "url": "https://www.sahilarora3117.github.io"
  },
  "icon": "icon.png",
  "keywords": [
    "Electron",
    "TV Shows",
    "Tracker",
    "React"
  ],
  "repository": "https://github.com/sahilarora3117/MyTVList",
  "category": "Utility",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    
    "electron-is-dev": "^1.2.0",
    "file-saver": "^2.0.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-file-picker": "0.0.6",
    "react-file-reader": "^1.1.4",
    "react-file-reader-input": "^2.0.0",
    "react-image": "^4.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.0.0",
    "serve": "^11.3.2",
    "wait-on": "^5.2.0"
  },
  "build": {
    "appId": "com.github.sahilarora3117.mytvlist",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "serve -s build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-package": "./node_modules/.bin/electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-package": "npm run build",
    "electron": "electron ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^18.3.7",
    "enzyme": "^3.11.0",
    "electron-builder": "^22.9.1",
    "enzyme-adapter-react-16": "^1.15.5"
  }
}
